{
  "name": "warden",
  "version": "2.2.0-next.6",
  "type": "module",
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "@keyv/sqlite": "^3.6.5",
    "@lavaclient/queue": "^2.1.1",
    "discord-api-types": "^0.37.42",
    "discord.js": "^14.11.0",
    "genversion": "^3.1.1",
    "google-tts-api": "^2.0.2",
    "keyv": "^4.5.2",
    "lavaclient": "^4.1.1",
    "lodash-es": "^4.17.21",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "rimraf": "^4.4.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "main": "dist/main.js",
  "scripts": {
    "slash:deploy": "node scripts/deploy-commands.js",
    "slash:delete": "node scripts/delete-commands.js",
    "build": "rimraf dist/ && genversion --es6 --semi src/lib/util/version.ts && tsc && tsc-alias",
    "start": "node .",
    "lint": "eslint . --ext .ts"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "release": {
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
